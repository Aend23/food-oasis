{
  "name": "foodoasis",
  "version": "1.0.35",
  "author": "Hack for LA",
  "description": "Web API Server for Food Oasis",
  "main": "server.js",
  "scripts": {
    "test": "mocha",
    "start": "nodemon server.js -ignore './client' ",
    "heroku-postbuild": "cd client && npm i && npm run build",
    "server": "nodemon server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint  \"**/*.{js,jsx}\"",
    "lint:fix": "eslint --fix  \"**/*.{js,jsx}\"",
    "release-notes": "gren release",
    "migrate": "node-pg-migrate -f db/config.js",
    "loadtest:local": "loadtest -t 60 -c 20 --rps 100 \"http://localhost:5000/api/stakeholderbests?name=&categoryIds[]=1&categoryIds[]=9&latitude=34.0544&longitude=-118.2439&distance=5&isInactive=false&verificationStatusId=0&tenantId=1\" ",
    "loadtest:dev": "loadtest -n 10000 -c 20 \"http://food-oasis-dev.herokuapp.com/api/stakeholderbests?name=&categoryIds[]=1&categoryIds[]=9&latitude=34.0544&longitude=-118.2439&distance=5&isInactive=false&verificationStatusId=0&tenantId=1\" ",
    "loadtest:prod": "loadtest -n 10000 -c 20  \"http://foodoasis.la/api/stakeholderbests?name=&categoryIds[]=1&categoryIds[]=9&latitude=34.0544&longitude=-118.2439&distance=5&isInactive=false&verificationStatusId=0&tenantId=1\" ",
    "loadtesttrivial:prod": "loadtest -t 60 -c 20 --rps 100 \"http://foodoasis.la/api/categories\" ",
    "loadtesttrivial:dev": "loadtest -t 60 -c 20 --rps 100 \"http://food-oasis-dev.herokuapp.com/api/categories\" "
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "license": "GPL-2.0",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "axios": "^0.19.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.4",
    "csv-parse": "^4.14.2",
    "csv-stringify": "^5.5.0",
    "dotenv": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "express-pino-logger": "^4.0.0",
    "json-2-csv": "^3.5.7",
    "jsonwebtoken": "^8.5.1",
    "massive": "^6.6.0",
    "mocha": "^8.1.3",
    "moment": "2.24.0",
    "multer": "^1.4.2",
    "node-pg-migrate": "^5.0.0",
    "nodemailer": "^6.3.1",
    "nodemon": "^2.0.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.3",
    "pino-noir": "^2.2.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "concurrently": "^5.0.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-flowtype": "^4.4.1",
    "eslint-plugin-import": "^2.18.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "2.0.5"
  }
}
